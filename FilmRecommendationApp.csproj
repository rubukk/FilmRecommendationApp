<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyTitle>Film Sitesi</AssemblyTitle>
    <AssemblyDescription>Film incelemeleri, fragmanlar ve kullanıcı yorumları içeren bir film sitesi.</AssemblyDescription>
    <AssemblyCompany>FilmSite Inc.</AssemblyCompany>
    <AssemblyProduct>FilmSite</AssemblyProduct>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(RunConfiguration)' == 'https' " />
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'http' " />

</Project>
